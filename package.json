{
  "name": "opencode-codex-auto-switch",
  "version": "1.0.0",
  "description": "OpenCode plugin for automatic ChatGPT account rotation with Codex backend - manage multiple ChatGPT Plus/Pro subscriptions with auto-failover",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "opencode",
    "openai",
    "codex",
    "chatgpt",
    "oauth",
    "multi-account",
    "auto-switch",
    "rotation",
    "plugin",
    "auth"
  ],
  "bin": {
    "codex-auto-switch": "./scripts/install.js"
  },
  "scripts": {
    "build": "tsc && cp lib/oauth-success.html dist/lib/",
    "typecheck": "tsc --noEmit",
    "install-config": "node scripts/install.js",
    "uninstall-config": "node scripts/install.js --uninstall"
  },
  "files": [
    "dist/",
    "config/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": "^1.0.150"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.0.150",
    "@opencode-ai/sdk": "^1.0.150",
    "@types/node": "^24.6.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@openauthjs/openauth": "^0.4.3",
    "jsonc-parser": "^3.3.1"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
